{
    "version": 2,
    "env": {
        "NLTK_DATA": "nltk_data"
    },
    "build": {
        "env": {
            "PIP_INSTALL_OPTIONS": "--no-deps"
        }
    },
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}